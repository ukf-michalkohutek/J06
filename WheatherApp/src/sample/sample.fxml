<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.VBox?>

<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" hgap="10" vgap="10" style="-fx-background-color: linear-gradient(from 25% 25% to 100% 100%, #F000, #241d37)">



    <VBox>
        <VBox>
            <Label  text="NITRA" style="-fx-font-size: 40"/>
            <Label fx:id="firstDate" style="-fx-font-size: 40"/>
            <Label fx:id="secondDate" style="-fx-font-size: 40"/>
            <Label fx:id="thirdDate" style="-fx-font-size: 40"/>
            <Label text="------------------------------------------------------------" style="-fx-font-size: 40"/>
            <Label fx:id="city" style="-fx-font-size: 40"/>
            <Label fx:id="cityFirst" style="-fx-font-size: 40"/>
            <Label fx:id="citySecond" style="-fx-font-size: 40"/>
            <Label fx:id="cityThird" style="-fx-font-size: 40"/>

        </VBox>

        <HBox spacing="10" alignment="bottom_left" >
            <TextField fx:id="CommandPane" promptText="Write command" prefWidth="400"/>
            <Button text="Execute" onAction="#execute"/>
        </HBox>
    </VBox>


</GridPane>